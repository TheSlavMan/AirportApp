        </activity>
        <activity android:name=".MainmenuActivity" />
        <activity android:name=".EmergencyReport" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".WorkSchedule" />
        <activity android:name=".FlightActivity" />
        <activity android:name=".PassengerActivity" />
        <activity android:name=".EmployeeActivity" />
        <activity android:name=".LuggageActivity" />
